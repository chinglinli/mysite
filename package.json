    "redux-segment": "^1.6.1",
    "reselect": "^3.0.0",
    "serialize-javascript": "^1.3.0",
    "yargs": "^8.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "html"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
